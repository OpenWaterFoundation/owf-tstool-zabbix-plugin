# Simple example to graph important system resources.
#
# CPU Utilization
ReadZabbix(DataStore="TriLynxZabbix",DataType="CPU: utilization-Avg",Interval="Hour",DataSource="Clients/CO-District-MHFD",LocId="ud-ns518b")
ReadZabbix(DataStore="TriLynxZabbix",DataType="CPU: utilization-Min",Interval="Hour",DataSource="Clients/CO-District-MHFD",LocId="ud-ns518b")
ReadZabbix(DataStore="TriLynxZabbix",DataType="CPU: utilization-Max",Interval="Hour",DataSource="Clients/CO-District-MHFD",LocId="ud-ns518b")
ReadZabbix(DataStore="TriLynxZabbix",DataType="CPU: utilization",Interval="IrregSecond",DataSource="Clients/CO-District-MHFD",LocId="ud-ns518b")
#
# Process graph.
ProcessTSProduct(TSProductFile="example-MHFD-cpu.tsp")
