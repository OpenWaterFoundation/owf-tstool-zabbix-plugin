# Memory Utilization
ReadZabbix(DataStore="TriLynxZabbix",DataType="Memory: Utilization-Avg",Interval="Hour",DataSource="Clients/CO-District-MHFD",LocId="ud-ns518b")
ReadZabbix(DataStore="TriLynxZabbix",DataType="Memory: Utilization-Min",Interval="Hour",DataSource="Clients/CO-District-MHFD",LocId="ud-ns518b")
ReadZabbix(DataStore="TriLynxZabbix",DataType="Memory: Utilization-Max",Interval="Hour",DataSource="Clients/CO-District-MHFD",LocId="ud-ns518b")
ReadZabbix(DataStore="TriLynxZabbix",DataType="Memory: Utilization",Interval="IrregSecond",DataSource="Clients/CO-District-MHFD",LocId="ud-ns518b")
